{
  "name": "jakemulley.github.io",
  "version": "0.0.3",
  "description": "A static site for jakemulley.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakemulley/jakemulley.github.io.git"
  },
  "scripts": {
    "dev:css": "npx tailwindcss -i ./src/_assets/css/main.css -o ./docs/css/main.css --watch",
    "dev:html": "npx @11ty/eleventy --serve",
    "dev": "concurrently npm:dev:*",
    "build:css": "npx tailwindcss -i ./src/_assets/css/main.css -o ./docs/css/main.css --minify",
    "build:html": "npx @11ty/eleventy",
    "build": "concurrently npm:build:*"
  },
  "author": "Jake Mulley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jakemulley/jakemulley.github.io/issues"
  },
  "homepage": "https://github.com/jakemulley/jakemulley.github.io#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@shockpkg/icon-encoder": "^2.1.3",
    "@tailwindcss/typography": "^0.5.8",
    "concurrently": "^7.5.0",
    "heroicons": "^2.0.13",
    "html-minifier-terser": "^7.0.0",
    "pretty": "^2.0.0",
    "prismjs": "^1.29.0",
    "sharp": "^0.31.2",
    "simple-icons": "^7.20.0",
    "tailwindcss": "^3.2.4"
  },
  "browserslist": [
    "defaults"
  ]
}
